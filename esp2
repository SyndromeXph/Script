local a=workspace.Players:FindFirstChild("Survivors")local b=workspace.Players:FindFirstChild("Killers")local c=13;local function d(e,f,g)local h=e:FindFirstChild("Head")if not h then return end;if h:FindFirstChild("NameESP")then return end;local i=Instance.new("BillboardGui")i.Name="NameESP"i.Adornee=h;i.Size=UDim2.new(0,200,0,50)i.StudsOffset=Vector3.new(0,2,0)i.AlwaysOnTop=true;i.Parent=h;local j=Instance.new("TextLabel")j.Size=UDim2.new(1,0,1,0)j.Position=UDim2.new(0,0,0,0)j.BackgroundTransparency=1;j.Text=f;j.TextColor3=Color3.new(0,0,0)j.TextStrokeTransparency=0;j.TextStrokeColor3=Color3.new(0,0,0)j.Font=Enum.Font.Arial;j.TextSize=c;j.Parent=i;local k=j:Clone()k.TextColor3=g;k.TextStrokeTransparency=1;k.Parent=i end;local function l(m,g)for n,e in ipairs(m:GetChildren())do if e:IsA("Model")and e:FindFirstChild("Head")then d(e,e.Name,g)end end;m.ChildAdded:Connect(function(o)if o:IsA("Model")then o:WaitForChild("Head",5)d(o,o.Name,g)end end)end;if a then l(a,Color3.new(1,1,1))end;if b then l(b,Color3.new(1,0,0))end
